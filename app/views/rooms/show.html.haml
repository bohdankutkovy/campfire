.form-group
  %h1 Chat room
.form-group
  #panel-body
    #messages
      = render @messages

.form-group
  .row
    .col-md-5
      %form
        %label Your Message:
        %br
        .input-group
          %input.form-control{placeholder: 'Type your message...', type: "text", 'data-behavior': "room_speaker"}
          %span.input-group-btn
            %a.btn.btn-success{:type => "button", :href => "#"} Send



